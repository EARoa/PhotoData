
  <div class="thumbpad">
    <% @photos.each do |photo| %>
    <% if photo.user_id == current_user. id %>
    <div class=" thumbnail col-sm-2">
      <%= image_tag photo.photo.url(:thumb), class: "img-responsive", "data-toggle"=>"modal", "data-target"=>"#modal-#{photo.id}" %>
      <%= photo.name %>
    </div>
  </div>


<div class="modal fade" id="modal-<%=photo.id%>" tabindex="-1" role="dialog" aria-labelledby="myLargeModalLabel" aria-hidden="true">
  <div class="modal-dialog">
    <div class="modal-content">
      <%= render partial: 'details', locals: {photo: photo} %>
    </div>
  </div>
</div>

<%end%>
<% end %>
